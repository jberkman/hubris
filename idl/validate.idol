// Validation API

Interface(
    name: "Validate",
    ops: {
        "validate_i2c": (
            args: {
                "index": "usize",
            },
            reply: Result(
                ok: (type : "ValidateOk", recv: FromPrimitive("u8")),
                err: CLike("ValidateError"),
            ),
        ),
        "selected_mux_segment": (
            args: {
                "index": "usize",
            },
            reply: Result(
                ok: "[u8; 2]",
                err: CLike("ValidateError"),
            ),
        ),
    },
)
